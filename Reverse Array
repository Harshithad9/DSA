def reverse_array(arr):
    left = 0
    right = len(arr) - 1
    while left < right:
        arr[left], arr[right] = arr[right], arr[left]
        left += 1
        right -= 1
    return arr
arr = [5,6,7,3,8,6,7,9]
print(reverse_array(arr))

----------------------------OR---------------------------

arr = [5,6,7,3,8,6,7,9]
reverse_arr = arr[::-1]
print(reverse_arr)
